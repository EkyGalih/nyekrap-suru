generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model ScrapeCache {
  id        Int      @id @default(autoincrement())
  endpoint  String
  cacheKey  String   @unique @map("cachekey")
  data      Json
  scrapedAt DateTime @default(now()) @map("scrapedat")
  expiresAt DateTime @map("expiresat")

  @@index([endpoint])
  @@map("ScrapeCache")
}
