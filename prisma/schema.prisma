generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model ScrapeCache {
  id        Int      @id @default(autoincrement())
  endpoint  String
  cacheKey  String   @unique
  data      Json
  scrapedAt DateTime @default(now()) @db.Timestamp(6)
  expiresAt DateTime @db.Timestamp(6)

  @@index([endpoint])
}
